<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:layout_width="407dp"
        android:layout_height="501dp"
        android:layout_margin="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:layout_width="388dp"
                android:layout_height="343dp"
                android:layout_margin="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView21TelephoneWorker">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayoutInfoWorker"
                    android:layout_width="match_parent"
                    android:layout_height="400dp"
                    android:visibility="gone">

                    <Spinner
                        android:id="@+id/spinner4ChargeWorker"
                        android:layout_width="250dp"
                        android:layout_height="53dp"
                        android:layout_marginStart="42dp"
                        android:layout_marginTop="12dp"
                        app:layout_constraintStart_toEndOf="@+id/textView9"
                        app:layout_constraintTop_toBottomOf="@+id/textView8BossWorker"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <Spinner
                        android:id="@+id/spinner5workPlaceWorker"
                        android:layout_width="250dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintStart_toEndOf="@+id/textView10"
                        app:layout_constraintTop_toBottomOf="@+id/spinner4ChargeWorker"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="20dp"
                        android:text="Resposable :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/spinner2RestaurantWorker" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="24dp"
                        android:text="Puesto en :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/spinner4ChargeWorker" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="28dp"
                        android:text="Cargo :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView8BossWorker" />

                    <TextView
                        android:id="@+id/textView8BossWorker"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:text="TextView"
                        android:textAlignment="gravity"
                        app:layout_constraintStart_toEndOf="@+id/textView7"
                        app:layout_constraintTop_toBottomOf="@+id/spinner2RestaurantWorker" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="16dp"
                        android:text="Restaurante :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Spinner
                        android:id="@+id/spinner2RestaurantWorker"
                        android:layout_width="250dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintStart_toEndOf="@+id/textView6"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayoutCalendar"
                    android:layout_width="393dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <Spinner
                        android:id="@+id/spinnerStatusWorker"
                        android:layout_width="178dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="48dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintStart_toEndOf="@+id/textView5"
                        app:layout_constraintTop_toBottomOf="@+id/editTextDateWorker"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <EditText
                        android:id="@+id/editTextDateWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:ems="10"
                        android:hint="01/01/2021"
                        android:inputType="date"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toEndOf="@+id/textView4"
                        app:layout_constraintTop_toBottomOf="@+id/editTextContracWorker" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="28dp"
                        android:text="Fecha de Alta :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextContracWorker" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="20dp"
                        android:text="Estado :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextDateWorker" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="24dp"
                        android:text="JORNADA :"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/editTextContracWorker"
                        android:layout_width="63dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="8dp"
                        android:ems="10"
                        android:hint="Horas"
                        android:inputType="textPersonName"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.119"
                        app:layout_constraintStart_toEndOf="@+id/textView3"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayoutTelephone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/editTextPhoneWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:drawableLeft="@android:drawable/ic_menu_call"
                        android:ems="10"
                        android:inputType="phone"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayoutEmail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/editTextEmailWorker"
                        android:layout_width="225dp"
                        android:layout_height="49dp"
                        android:layout_marginStart="8dp"
                        android:drawableLeft="@android:drawable/ic_dialog_email"
                        android:ems="10"
                        android:hint="XXXXXXX@gmail.com"
                        android:inputType="textPersonName"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TouchTargetSizeCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayoutDates"
                    android:layout_width="wrap_content"
                    android:layout_height="314dp"
                    android:visibility="visible">

                    <EditText
                        android:id="@+id/editTextNameWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:drawableLeft="@android:drawable/ic_menu_edit"
                        android:ems="10"
                        android:hint="Nombre"
                        android:inputType="textPersonName"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TouchTargetSizeCheck" />

                    <EditText
                        android:id="@+id/editTextSurnameWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:drawableLeft="@android:drawable/ic_menu_edit"
                        android:ems="10"
                        android:hint="Apellido 1"
                        android:inputType="textPersonName"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextNameWorker"
                        tools:ignore="TouchTargetSizeCheck" />

                    <EditText
                        android:id="@+id/editTextSurname2Worker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:drawableLeft="@android:drawable/ic_input_add"
                        android:ems="10"
                        android:hint="Apellido 2"
                        android:inputType="textPersonName"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextSurnameWorker" />

                    <EditText
                        android:id="@+id/editTextNifWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:drawableLeft="@android:drawable/ic_input_add"
                        android:ems="10"
                        android:hint="NIF"
                        android:inputType="textPersonName"
                        android:minHeight="48dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextSurname2Worker" />

                    <EditText
                        android:id="@+id/editTextTextAdressWorker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:ems="10"
                        android:hint="Direccion"
                        android:inputType="textPersonName"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editTextNifWorker" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <ImageView
                android:id="@+id/imageView22CalendarWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toEndOf="@+id/imageView21TelephoneWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/calendario" />

            <ImageView
                android:id="@+id/imageView21TelephoneWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toEndOf="@+id/imageView19DatesWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/llamada_telefonica" />

            <ImageView
                android:id="@+id/imageView23InfoWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toEndOf="@+id/imageView22CalendarWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/camarero" />

            <ImageView
                android:id="@+id/imageView20TrashWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toEndOf="@+id/imageView23InfoWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/trash" />

            <ImageView
                android:id="@+id/imageView19DatesWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toEndOf="@+id/imageView18EmailWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/nif" />

            <ImageView
                android:id="@+id/imageView18EmailWorker"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toEndOf="@+id/imageView17PhotoWorker"
                app:layout_constraintTop_toBottomOf="@+id/imageView17PhotoWorker"
                app:srcCompat="@drawable/email" />

            <ImageView
                android:id="@+id/imageView17PhotoWorker"
                android:layout_width="97dp"
                android:layout_height="91dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/photo_camera" />

            <TextView
                android:id="@+id/textView2NameCompletWorker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="TextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.068"
                app:layout_constraintStart_toEndOf="@+id/imageView17PhotoWorker"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>